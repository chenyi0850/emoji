#editPhoto{
  height: 100vh;
  width: 100%;
  background: #5F9EA0;
  padding-top: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.img{
  display: block;
  width: 100%;
  height: 100%;
  margin: 0 auto;
}
#ipt{
  border: 1rpx white solid;
  width: 300px;
  margin:15rpx 0;
  height: 60rpx;
  font-size: 55rpx;
}
#top{
  display: flex;
  flex-direction: column;
  flex:1;
  align-items: center;
  justify-content: center;
}
#mv{
  min-width: 50%;
  
  height: 75rpx;
}
#mv input{
  height: 100%;
  font-size: 75rpx;
}
#bottom{
  border-top: 1rpx rgb(224, 217, 217) solid;
  display: flex;
  width: 100%;
  background: rgb(34, 2, 71);
  color: rgb(226, 224, 224);
}
.btn{
  width: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 18rpx 0;
  font-size: 25rpx;
}
.btn image{
  width: 40%;
}
#color-area{
  display: flex;
  justify-content: space-around;
}
.color{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}